e":38,"column":28}},{"generated":{"line":113,"column":102},"source":"Components/Jokes.js","original":{"line":37,"column":24}},{"generated":{"line":114,"column":0},"source":"Components/Jokes.js","original":{"line":40,"column":21}},{"generated":{"line":114,"column":7},"source":"Components/Jokes.js","original":{"line":36,"column":20}},{"generated":{"line":114,"column":8},"source":"Components/Jokes.js","original":{"line":30,"column":12}},{"generated":{"line":114,"column":9},"source":"Components/Jokes.js","original":{"line":29,"column":8}},{"generated":{"line":115,"column":0},"source":"Components/Jokes.js","original":{"line":44,"column":5}},{"name":"Component","generated":{"line":119,"column":2},"source":"Components/Jokes.js","original":{"line":3,"column":20}},{"name":"Jokes","generated":{"line":121,"column":15},"source":"Components/Jokes.js","original":{"line":47,"column":15}}],"sources":{"Components/Jokes.js":"import React, { Component } from 'react';\n\nclass Jokes extends Component {\n    state = ({ joke:{}, moreJokes:false, jokes:[] });\n\n    componentDidMount() {\n        fetch('https://official-joke-api.appspot.com/random_joke')\n        .then((response) => { return response.json(); })\n        .then((data) => {\n            console.log('Response Data = ', data)\n            this.setState({ joke: data }) \n        })\n        .catch((failed) => { alert(failed.message) })\n    }\n\n    // Class Property syntax - allows to access 'this' object on the compoent \n    fetchJokes = () => {\n        fetch('https://official-joke-api.appspot.com/random_ten')\n            .then((response) => { return response.json() })\n            .then((data) => { \n                console.log('data =', data);\n                this.setState({ jokes: data }) \n            })\n            .catch((failed) => { alert(failed.message) })\n    }\n\n    render() {\n        const { setup, punchline } = this.state.joke;\n        return (\n            <div>\n                <h3>Highlighted Joke</h3>\n                <p>{setup} --> <em>{punchline}</em></p>\n\n                <button onClick={this.fetchJokes} style={{marginTop: 30}} className='button'>More Jokes</button>\n                {\n                    this.state.jokes.map((joke) => {\n                        return (\n                            <p key={joke.id} style={{margin: 20}}>{joke.setup} --> <em>{joke.punchline}</em></p>\n                        )\n                    })\n                }\n            </div>\n        )\n    }\n}\n\nexport default Jokes;   "},"lineCount":null}},"hash":"b7546a1edea06d7cb9a80ce246597890","cacheData":{"env":{}}}